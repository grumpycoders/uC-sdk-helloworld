{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "launch",
            "name": "Debug gdb",
            "gdbpath": "/usr/local/bin/arm-none-eabi-gdb",
            "target": "./test.elf",
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "OpenOCD",
            "postDebugTask": "Stop OpenOCD",
            "autorun": [
                "target remote localhost:3333",
                "symbol-file ./test.elf",
                "interrupt",
                "monitor reset halt",
                "load ./test.elf",
                "tbreak main"
            ],
            "valuesFormatting": "parseText"
        }
    ]
}